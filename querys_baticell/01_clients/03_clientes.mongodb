// Poner en uso la base de datos
use("baticell_store")

// Listar todos los clientes que no procedan de Breña o La Molina
// sólo mostrar los atributos nombre, apellidos, email y suscripciones
db.clients.find(
    {
        "direccion": {
            $not: { $regex: "Breña|La Molina" }
        }
    },
    {
        "nombre": 1,
        "apellidos": 1,
        email: 1,
        suscripciones: 1,
        _id: 0
    }
)